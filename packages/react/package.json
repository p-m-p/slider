{
  "name": "@boxslider/react",
  "version": "1.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.tx",
  "private": false,
  "repository": {
    "type": "Git",
    "url": "https://github.com/boxslider/slider/packages/react"
  },
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production babel src/components --out-dir dist --extensions \".ts,.tsx\" --ignore src/stories && npm run types",
    "clean": "rimraf dist",
    "types": "tsc --project tsconfig.dist.json --emitDeclarationOnly --outDir dist",
    "prepare": "npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@boxslider/slider": "^1.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  }
}
